<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    android:id="@+id/activity_next"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fmedlin.intentbuildersample.org.fmedlin.intentbuildersample.MainActivity">

    <TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/activity_next"
        app:layout_constraintLeft_toLeftOf="@+id/activity_next"
        app:layout_constraintRight_toRightOf="@+id/activity_next"
        app:layout_constraintTop_toTopOf="@+id/activity_next"/>

</android.support.constraint.ConstraintLayout>
